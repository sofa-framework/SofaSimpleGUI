# CMake package configuration file for the SofaSimpleGUI plugin.
# It defines the following variables:
# - SofaSimpleGUI_INCLUDE_DIRS
# - SofaSimpleGUI_LIBRARIES

@PACKAGE_INIT@

set_and_check(SOFASIMPLEGUI_INCLUDE_DIR "@PACKAGE_SOFASIMPLEGUI_INCLUDE_DIR@")

check_required_components(SofaSimpleGUI)

find_package(SofaGeneral REQUIRED)

if(NOT TARGET SofaSimpleGUI)
	include("${CMAKE_CURRENT_LIST_DIR}/SofaSimpleGUITargets.cmake")
endif()

set(SofaSimpleGUI_LIBRARIES SofaSimpleGUI)
set(SofaSimpleGUI_INCLUDE_DIRS ${SOFASIMPLEGUI_INCLUDE_DIR} ${SofaGeneral_INCLUDE_DIRS})
